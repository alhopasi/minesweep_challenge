{
    "containerDefinitions": [
        {
            "name": "Minesweep",
            "image": "982081085329.dkr.ecr.eu-west-1.amazonaws.com/minesweep:latest",
            "cpu": 512,
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "MinesweepDataVolume",
                    "containerPath": "/data",
                    "readOnly": false
                },
                {
                    "sourceVolume": "MinesweepDataOnlineVolume",
                    "containerPath": "/data/online",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "family": "MinesweepTaskDefinition",
    "taskRoleArn": "arn:aws:iam::982081085329:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::982081085329:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "MinesweepDataVolume",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-029be98ca0037184d",
                "rootDirectory": "/data/"
            }
        },
        {
            "name": "MinesweepDataOnlineVolume",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-029be98ca0037184d",
                "rootDirectory": "/data/online/"
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}